{
  "name": "@open-mercato/vector",
  "version": "0.3.12",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "scripts": {
    "build": "node build.mjs"
  },
  "exports": {
    ".": "./dist/index.js",
    "./types": {
      "types": "./src/types.ts",
      "default": "./dist/types.js"
    },
    "./modules/vector/di": {
      "types": "./src/modules/vector/di.ts",
      "default": "./dist/modules/vector/di.js"
    },
    "./modules/vector/frontend": {
      "types": "./src/modules/vector/frontend/index.ts",
      "default": "./dist/modules/vector/frontend/index.js"
    },
    "./*.ts": {
      "types": "./src/*.ts",
      "default": "./dist/*.js"
    },
    "./*.tsx": {
      "types": "./src/*.tsx",
      "default": "./dist/*.js"
    },
    "./*.json": "./src/*.json",
    "./*": {
      "types": ["./src/*.ts", "./src/*.tsx"],
      "default": "./dist/*.js"
    },
    "./*/*.json": "./src/*/*.json",
    "./*/*": {
      "types": ["./src/*/*.ts", "./src/*/*.tsx"],
      "default": "./dist/*/*.js"
    },
    "./*/*/*.json": "./src/*/*/*.json",
    "./*/*/*": {
      "types": ["./src/*/*/*.ts", "./src/*/*/*.tsx"],
      "default": "./dist/*/*/*.js"
    },
    "./*/*/*/*.json": "./src/*/*/*/*.json",
    "./*/*/*/*": {
      "types": ["./src/*/*/*/*.ts", "./src/*/*/*/*.tsx"],
      "default": "./dist/*/*/*/*.js"
    },
    "./*/*/*/*/*.json": "./src/*/*/*/*/*.json",
    "./*/*/*/*/*": {
      "types": ["./src/*/*/*/*/*.ts", "./src/*/*/*/*/*.tsx"],
      "default": "./dist/*/*/*/*/*.js"
    },
    "./*/*/*/*/*/*.json": "./src/*/*/*/*/*/*.json",
    "./*/*/*/*/*/*": {
      "types": ["./src/*/*/*/*/*/*.ts", "./src/*/*/*/*/*/*.tsx"],
      "default": "./dist/*/*/*/*/*/*.js"
    }
  },
  "dependencies": {
    "@open-mercato/shared": "0.3.12",
    "@ai-sdk/amazon-bedrock": "^4.0.8",
    "@ai-sdk/cohere": "^3.0.4",
    "@ai-sdk/google": "^1.2.6",
    "@ai-sdk/mistral": "^3.0.5",
    "@ai-sdk/openai": "^3.0.5",
    "ai": "^6.0.0",
    "ai-sdk-ollama": "^3.0.1",
    "pg": "^8.16.3"
  }
}
