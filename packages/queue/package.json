{
  "name": "@open-mercato/queue",
  "version": "0.3.12",
  "description": "Multi-strategy job queue with local and BullMQ support",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "scripts": {
    "build": "node build.mjs"
  },
  "exports": {
    ".": "./dist/index.js",
    "./worker": {
      "types": "./src/worker/runner.ts",
      "default": "./dist/worker/runner.js"
    },
    "./*": {
      "types": ["./src/*.ts"],
      "default": "./dist/*.js"
    },
    "./*/*": {
      "types": ["./src/*/*.ts"],
      "default": "./dist/*/*.js"
    }
  },
  "peerDependencies": {
    "bullmq": "^5.0.0"
  },
  "peerDependenciesMeta": {
    "bullmq": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^20.0.0"
  }
}
