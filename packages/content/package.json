{
  "name": "@open-mercato/content",
  "version": "0.3.13",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "scripts": {
    "build": "node build.mjs",
    "test": "jest --passWithNoTests --config ../../jest.config.cjs"
  },
  "exports": {
    ".": "./dist/index.js",
    "./*.ts": {
      "types": "./src/*.ts",
      "default": "./dist/*.js"
    },
    "./*.tsx": {
      "types": "./src/*.tsx",
      "default": "./dist/*.js"
    },
    "./*.json": "./src/*.json",
    "./*": {
      "types": [
        "./src/*.ts",
        "./src/*.tsx"
      ],
      "default": "./dist/*.js"
    },
    "./*/*.json": "./src/*/*.json",
    "./*/*": {
      "types": [
        "./src/*/*.ts",
        "./src/*/*.tsx"
      ],
      "default": "./dist/*/*.js"
    },
    "./*/*/*.json": "./src/*/*/*.json",
    "./*/*/*": {
      "types": [
        "./src/*/*/*.ts",
        "./src/*/*/*.tsx"
      ],
      "default": "./dist/*/*/*.js"
    },
    "./*/*/*/*.json": "./src/*/*/*/*.json",
    "./*/*/*/*": {
      "types": [
        "./src/*/*/*/*.ts",
        "./src/*/*/*/*.tsx"
      ],
      "default": "./dist/*/*/*/*.js"
    },
    "./*/*/*/*/*.json": "./src/*/*/*/*/*.json",
    "./*/*/*/*/*": {
      "types": [
        "./src/*/*/*/*/*.ts",
        "./src/*/*/*/*/*.tsx"
      ],
      "default": "./dist/*/*/*/*/*.js"
    }
  },
  "dependencies": {
    "@open-mercato/shared": "workspace:*"
  }
}
