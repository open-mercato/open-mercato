{
  "name": "@open-mercato/cli",
  "version": "0.3.13",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./mercato": {
      "types": "./src/mercato.ts",
      "default": "./dist/mercato.js"
    },
    "./registry": {
      "types": "./src/registry.ts",
      "default": "./dist/registry.js"
    },
    "./*.ts": {
      "types": "./src/*.ts",
      "default": "./dist/*.js"
    },
    "./*": {
      "types": [
        "./src/*.ts"
      ],
      "default": "./dist/*.js"
    },
    "./*/*": {
      "types": [
        "./src/*/*.ts"
      ],
      "default": "./dist/*/*.js"
    },
    "./*/*/*": {
      "types": [
        "./src/*/*/*.ts"
      ],
      "default": "./dist/*/*/*.js"
    },
    "./*/*/*/*": {
      "types": [
        "./src/*/*/*/*.ts"
      ],
      "default": "./dist/*/*/*/*.js"
    }
  },
  "bin": {
    "mercato": "./bin/mercato"
  },
  "scripts": {
    "build": "node build.mjs"
  },
  "dependencies": {
    "@mikro-orm/core": "^6.6.2",
    "@mikro-orm/migrations": "^6.6.2",
    "@mikro-orm/postgresql": "^6.6.2",
    "@open-mercato/shared": "0.3.13",
    "pg": "^8.16.3",
    "typescript": "^5.9.3"
  }
}
