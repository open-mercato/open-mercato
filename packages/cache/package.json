{
  "name": "@open-mercato/cache",
  "version": "0.3.13",
  "description": "Multi-strategy cache service with tag-based invalidation support",
  "type": "module",
  "main": "./dist/index.js",
  "scripts": {
    "build": "node build.mjs",
    "test": "jest"
  },
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./dist/index.js"
    },
    "./types": {
      "types": "./src/types.ts",
      "default": "./dist/types.js"
    },
    "./*": {
      "types": [
        "./src/*.ts"
      ],
      "default": "./dist/*.js"
    },
    "./*/*": {
      "types": [
        "./src/*/*.ts"
      ],
      "default": "./dist/*/*.js"
    }
  },
  "keywords": [
    "cache",
    "caching",
    "redis",
    "sqlite",
    "memory",
    "tags",
    "invalidation"
  ],
  "peerDependencies": {
    "better-sqlite3": "^9.0.0",
    "ioredis": "^5.0.0"
  },
  "peerDependenciesMeta": {
    "better-sqlite3": {
      "optional": true
    },
    "ioredis": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.0",
    "@types/node": "^20.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
